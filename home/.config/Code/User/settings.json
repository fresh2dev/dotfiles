{
    "editor.renderWhitespace": "all",
    "files.associations": {
        "*.Rmd": "markdown",
        "*.yml": "yaml",
        "*.yaml": "yaml"
    },
    "explorer.confirmDragAndDrop": false,
    "files.hotExit": "off",
    "workbench.startupEditor": "newUntitledFile",
    "files.insertFinalNewline": false,
    "files.eol": "\n",
    "extensions.ignoreRecommendations": true,
    "workbench.sideBar.location": "left",
    "explorer.confirmDelete": false,
    "editor.cursorStyle": "line",
    "terminal.integrated.cursorStyle": "block",
    "terminal.integrated.defaultLocation": "editor",
    "security.workspace.trust.untrustedFiles": "open",
    "files.autoSave": "afterDelay",
    "yaml.schemaStore.enable": false,
    "redhat.telemetry.enabled": false,
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "jupyter.askForKernelRestart": false,
    "security.workspace.trust.enabled": false,
    "editor.tabSize": 4,
    "editor.fontFamily": "'Liberation Mono', monospace",
    // "terminal.integrated.fontFamily": "'FiraMono NF', monospace",
    "telemetry.telemetryLevel": "off",
    "powershell.scriptAnalysis.enable": true,
    "powershell.codeFormatting.preset": "Allman",
    "powershell.codeFormatting.alignPropertyValuePairs": false,
    "powershell.codeFormatting.pipelineIndentationStyle": "NoIndentation",
    "powershell.codeFormatting.ignoreOneLineBlock": false,
    "powershell.codeFormatting.openBraceOnSameLine": false,
    "powershell.codeFormatting.useCorrectCasing": true,
    "powershell.codeFormatting.useConstantStrings": true,
    "powershell.codeFormatting.autoCorrectAliases": true,
    "powershell.codeFormatting.trimWhitespaceAroundPipe": true,
    "powershell.codeFormatting.whitespaceBetweenParameters": true,
    "powershell.codeFormatting.avoidSemicolonsAsLineTerminators": true,
    "powershell.codeFormatting.addWhitespaceAroundPipe": true,
    "powershell.codeFormatting.newLineAfterCloseBrace": true,
    "powershell.codeFormatting.newLineAfterOpenBrace": true,
    "powershell.codeFormatting.whitespaceAfterSeparator": true,
    "powershell.codeFormatting.whitespaceAroundOperator": true,
    "powershell.codeFormatting.whitespaceBeforeOpenBrace": true,
    "powershell.codeFormatting.whitespaceBeforeOpenParen": true,
    "powershell.codeFormatting.whitespaceInsideBrace": true,
    "powershell.sideBar.CommandExplorerVisibility": false,
    "powershell.integratedConsole.suppressStartupBanner": true,
    "powershell.debugging.createTemporaryIntegratedConsole": true,
    "powershell.startAsLoginShell.linux": true,
    "terminal.external.windowsExec": "C:\\Program Files\\PowerShell\\7\\pwsh.exe",
    "terminal.integrated.enableMultiLinePasteWarning": false,
    "powershell.promptToUpdatePowerShell": false,
    "[powershell]": {},
    "powershell.powerShellAdditionalExePaths": {
        "Powershell v7": "C:\\Program Files\\PowerShell\\7\\pwsh.exe",
        "Powershell v5": "C:\\WINDOWS\\System32\\WindowsPowerShell\\v1.0\\powershell.exe"
    },
    "terminal.integrated.defaultProfile.windows": "pwsh",
    "terminal.integrated.defaultProfile.linux": "zsh",
    "window.menuBarVisibility": "toggle",
    "git.branchProtection": [
        "main",
        "master"
    ],
    "markdown.marp.enableHtml": true,
    "markdown.marp.exportType": "html",
    "partialDiff.enableTelemetry": false,
    "powershell.powerShellDefaultVersion": "Powershell v5",
    "git.mergeEditor": true,
    "window.commandCenter": 1,
    "git.enableSmartCommit": true,
    "markdown.extension.print.absoluteImgPath": false,
    "markdown.extension.toc.levels": "2..3",
    "editor.acceptSuggestionOnEnter": "smart",
    "editor.fontSize": 15,
    "editor.lineHeight": 1.35,
    "diffEditor.ignoreTrimWhitespace": false,
    "search.useGlobalIgnoreFiles": true,
    "search.useParentIgnoreFiles": true,
    "terminal.integrated.gpuAcceleration": "off",
    "git.requireGitUserConfig": false,
    "git.confirmSync": false,
    "git.autofetch": false,
    "python.terminal.activateEnvInCurrentTerminal": false,
    "python.terminal.activateEnvironment": false,
    "search.useIgnoreFiles": true,
    "keyboard.dispatch": "keyCode",
    "ruff.importStrategy": "fromEnvironment",
    "jupyter.interactiveWindow.creationMode": "perFile",
    "workbench.editor.empty.hint": "hidden",
    "git.openRepositoryInParentFolders": "never",
    "editor.wordWrap": "on",
    "vs-kubernetes": {
        "vs-kubernetes.crd-code-completion": "enabled"
    },
    "open-in-vim.useNeovim": false,
    "editor.autoClosingBrackets": "never",
    "editor.autoClosingQuotes": "never",
    "explorer.compactFolders": false,
    "vim.targets.enable": true,
    "vim.vimrc.enable": true,
    "vim.highlightedyank.enable": true,
    "vim.highlightedyank.duration": 666,
    "vim.leader": ",",
    "vim.useSystemClipboard": true,
    "vim.visualstar": true,
    "codeium.enableInComments": false,
    "codeium.enableSearch": false,
    "codeium.enableConfig": {
        "*": false
    },
    "codeium.enableCodeLens": false,
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
    "window.zoomLevel": 1,
    "continue.enableTabAutocomplete": false,
    "continue.telemetryEnabled": false
}
